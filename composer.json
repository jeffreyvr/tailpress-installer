{
  "name": "jeffreyvanrossum/tailpress-installer",
  "description": "TailPress installer package.",
  "license": "MIT",
  "authors": [
    {
      "name": "Jeffrey van Rossum",
      "email": "jeffrey@vanrossum.dev"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.4|^8.0|^8.1",
    "symfony/console": "^4.0|^5.0|^6.0",
    "symfony/process": "^4.2|^5.0|^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.3",
    "spatie/ray": "dev-main"
  },
  "bin": [
    "bin/tailpress"
  ],
  "autoload": {
    "psr-4": {
      "Jeffreyvr\\TailPressInstaller\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jeffreyvr\\TailPressInstaller\\Console\\Tests\\": "tests/"
    }
  }
}
